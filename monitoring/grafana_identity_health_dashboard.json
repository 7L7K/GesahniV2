                "type": "stat",
                "targets": [
                    {
                        "expr": "token_encrypt_bytes_total + token_decrypt_bytes_total",
                        "legendFormat": "Total Bytes"
                    }
                ],
                "fieldConfig": {
                    "defaults": {
                        "color": {
                            "mode": "palette-classic"
                        },
                        "unit": "bytes"
                    }
                },
                "gridPos": {
                    "h": 4,
                    "w": 6,
                    "x": 18,
                    "y": 16
                }
            }
        ],
        "time": {
            "from": "now-1h",
            "to": "now"
        },
        "refresh": "30s",
        "schemaVersion": 27,
        "version": 1,
        "links": []
